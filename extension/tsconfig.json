{
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "react",
    "jsxFactory": "vscpp",
    "jsxFragmentFactory": "vscppf",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "rootDir": "src",
    "module": "Node16",
    "strict": true, /* enable all strict type-checking options */
    "outDir": "dist",
    "sourceMap": true
  },
  "include": [
    "src/**/*"
  ]
}
